{
  "name": "ResponsiveWeb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-web": "browser-sync start -s 'src' -f 'src'",
    "scss": "node-sass --output-style expanded -o src/css/ src/scss/ | postcss -u autoprefixer -r src/css/*",
    "watch-scss": "onchange './src/scss/*.scss' -- npm run scss",
    "babel": "babel src/js/mainES6.js -o src/js/main.js",
    "build": "babel src -d lib",
    "watch-js": "onchange './src/js/mainES6.js' -- npm run babel",
    "watchAll": "npm-run-all --parallel watch-*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^4.0.0-beta.3",
    "browser-sync": "^2.23.3",
    "director": "^1.2.8",
    "eslint": "^4.13.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.6.1",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.2",
    "popper.js": "^1.12.9",
    "postcss-bem": "^0.4.1",
    "postcss-cli": "^4.1.1",
    "sass": "^1.0.0-beta.3"
  }
}
