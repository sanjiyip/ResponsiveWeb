{
  "name": "ResponsiveWeb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "build-o": "webpack",
    "dev": "webpack-dev-server",
    "build-web": "browser-sync start -s 'src' -f 'src'",
    "scss":
      "node-sass --output-style expanded -o src/css/ src/scss/ | postcss -u autoprefixer -r src/css/*",
    "watch-scss": "onchange './src/scss/*.scss' -- npm run scss",
    "babel": "babel src/js/mainES6.js -o src/js/main.js",
    "watch-js": "onchange './src/js/mainES6.js' -- npm run babel",
    "watchAll": "npm-run-all --parallel watch-*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^4.0.0-beta.3",
    "browser-sync": "^2.23.3",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.8",
    "director": "^1.2.8",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.30.1",
    "html-withimg-loader": "^0.1.16",
    "jquery": "^3.2.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.2.1",
    "owl.carousel": "^2.2.0",
    "parallelshell": "^3.0.2",
    "popper.js": "^1.12.9",
    "postcss-bem": "^0.4.1",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^2.0.10",
    "sass": "^1.0.0-beta.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  }
}
